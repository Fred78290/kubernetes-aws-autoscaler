{
  "kind": "ClusterRole",
  "apiVersion": "rbac.authorization.k8s.io/v1",
  "metadata": {
    "labels": {
      "k8s-app": "kubernetes-dashboard"
    },
    "name": "kubernetes-dashboard"
  },
  "rules": [
    {
      "apiGroups": [
        "metrics.k8s.io"
      ],
      "resources": [
        "pods",
        "nodes"
      ],
      "verbs": [
        "get",
        "list",
        "watch"
      ]
    }
  ]
}
